<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ar.com.gmeventas.entities.TicketTime" schema="gaston" table="ticket_time">
        <id name="id" column="ID">
            <generator class="identity"/>
        </id>
        <property name="fecha" type="date" column="FECHA"/>
        <property name="hora" type="integer" column="HORA"/>
        <property name="minuto" type="integer" column="MINUTO"/>
        <property name="segundo" type="integer" column="SEGUNDO"/>
        <property name="token" type="string" column="TOKEN"/>
        <property name="sign" type="string" column="SIGN"/>
    </class>
</hibernate-mapping>